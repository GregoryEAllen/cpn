		OS_LIBS += -lrt
	endif
endif
#------------------------------------ USE_PTHREADS
ifdef USE_PTHREADS
	ifneq (,$(findstring HPUX_1020,$(shell echo `uname -s | sed 's/-//g'`_`uname -r | sed 's/[^0-9]//g'`)))
		OS_INCDIR += -I${PATHTOROOT}/vendors/open-source/pth
		OS_LIBS += -L${PATHTOROOT}/vendors/open-source/pth -lpth_hpux_1020
	else
		OS_LIBS += -lpthread
	endif
endif

